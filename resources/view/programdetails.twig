{% extends "templates/app.twig"%}

{% block content %}
    <section id="main">
        <div class="breadcrumb-wrapper">
            <div class="pattern-overlay">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-xs-12 col-sm-6">
                            <h2 class="title">{{programDetails.programs| title}}</h2>
                        </div>
                        <div class="col-lg-6 col-md-6 col-xs-12 col-sm-6">
                            <div class="breadcrumbs pull-right">
                                <ul>
                                    <li>You are Now on:</li>
                                    <li><a href="{{path_for('home')}}">Home</a></li>
                                    <li><a href="{{path_for('technoclub')}}">TechnoClub @Schools</a></li>
                                    <li>Program Deatils</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Main Content -->
        <div class="content margin-top60 margin-bottom60">
            <div class="container">
                <!--<div class="row">
                    <div class="posts-block col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <img src="img/about-2.jpg" alt="about">
                    </div>
                </div>-->
                <div class="row">
                    <div class="posts-block col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <article>
                            <h2>{{programDetails.programs| title}}</h2>
                            <div>
                                 <div class="col-sm-12 col-md-12 col-lg-12">
                        <div class="my-account">
                            <div class="table-responsive">
                                <table class="table table-striped table-responsive table-bordered text-left my-orders-table">
                                    <thead>
                                        <tr>                                          
                                            <th>Programs</th>
                                            <td>{{programDetails.programs| title}}</td>
                                        </tr>
                                         <tr>
                                            <th>Start Date</th>
                                            <td>{{programDetails.start_date| date('d/M/Y')}}</td>
                                        </tr>
                                         <tr>
                                            <th>End Date</th>
                                            <td>{{programDetails.end_date| date('d/M/Y')}}</td>
                                        </tr>
                                         <tr>
                                            <th>Time</th>
                                            <td>{{programDetails.time}}</td>
                                        </tr>
                                         <tr>
                                            <th>Grade</span></th>
                                            <td>{{programDetails.grade}}</td>
                                        </tr>
                                         <tr>
                                            <th>Day</th>
                                            <td>{{programDetails.day}}</td>
                                        </tr>
                                         <tr>
                                            <th>Seats</th>
                                            <td>{{programDetails.seat}}</td>
                                        </tr>
                                        <tr>
                                            <th>Fee</th>
                                            <td>{{programDetails.fee}}</td>
                                        </tr>
                                         <tr>
                                            <th>Details</th>
                                            <td>{{programDetails.description}}</td>
                                        </tr>
                                         
                                    </thead>
                                    <tbody>
                                    </tbody>
                                </table>
                            </div>
                            <a href="{{path_for('studentregister')}}" class="btn btn-color text-capitalize">register</a>
                        </div>
                    </div>
                            </div>
                        </article>
                    </div>
                    <!-- Left Section End -->
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /Main Content -->
</section>
{% endblock %}